msgid "  --config <file>  Use an alternate config file (instead of '%s')"
msgstr ""
"  --config <file>  Χρήση εναλλακτικού αρχείου ρυθμίσεων (αντί του '%s')"

#: scripts/makepkg.sh.in:1168
