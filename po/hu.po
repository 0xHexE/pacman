#: src/pacman/package.c:89
#, c-format
msgid "Install Date   : %s %s\n"
msgstr "Telepítés ideje : %s %s\n"

#: src/pacman/package.c:90
#, c-format
msgid "Install Reason : %s\n"
msgstr "Telepítés oka   : %s\n"

